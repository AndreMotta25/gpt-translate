{
    "manifest_version": 3,
    "name": "Translate-GPT",
    "version": "1.0",
    "description": "Translate web page content with chatGPT",
    "content_scripts": [ 
      { 
        "js": ["assets/script.js"],
        "css":["styles/style.css"], 
        "matches": [ 
          "https://*/*" 
        ] 
      } 
    ],
    "permissions": ["activeTab","scripting"],
    "icons":{
      "16":"images/icon-16.png",
      "32":"images/icon-32.png",
      "48":"images/icon-48.png",
      "128":"images/icon-128.png"
    }
  }